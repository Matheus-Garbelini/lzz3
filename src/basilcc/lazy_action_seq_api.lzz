namespace basilcc
{
  class CC;

  // get lazy action seq from actions
  LazyActionSeqPtr getLazyActionSeq (CC & cc, LazyActionPtrVector & lazy_actions)
  {
    return getTable <LazyActionSeqTable> (cc).getLazyActionSeq (lazy_actions);
  }
}

